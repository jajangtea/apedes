<com:TContent ID="maincontent">
    <com:TPanel Visible="<%= $this->getDefaultProcess(false) %>" CssClass="content">	   
        <div class="panel panel-flat">
            <div class="panel-heading">
                <h5 class="panel-title"><i class="icon-googleplus5 position-left"></i> BUKU INDUK PENDUDUK <com:TLabel ID="lblAddKurikulum" /></h5>
                <div class="heading-elements">
                    <ul class="icons-list">
                        <li>
                            <a href="<%=$this->Page->constructUrl('KonversiMatakuliah',true);%>" data-action="closeredirect"></a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="panel-body">
                <div class="form-horizontal">
                    <div class="form-group" id="divAddNomorUrut">
                        <label class="col-md-2 control-label text-right">Nomor Urut :</label>
                        <div class="col-md-10">
                            <com:TTextBox ID="txtAddNomorUrut" MaxLength="6" CssClass="form-control" Width="80px" />
                            <com:TRequiredFieldValidator ID="FieldAddNimAsalValidator" ControlToValidate="txtAddNomorUrut" Display="Dynamic" ErrorMessage="Isi NIM Asal" ValidationGroup="addKonversi" CssClass="has-error help-block">
                                <prop:ClientSide.OnValidationError>
                                    $('divAddNomorUrut').addClassName('has-error');
                                    jQuery('#<%=$this->FieldAddNimAsalValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAddNomorUrut').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>
                    <div class="form-group" id="divAddNamaLengkap">
                        <label class="col-md-2 control-label text-right">Nama Lengkap/Panggilan :</label>
                        <div class="col-md-4">
                            <com:TTextBox ID="txtAddNamaLengkap" CssClass="form-control" />
                            <com:TRequiredFieldValidator ID="FieldAddNamaValidator" ControlToValidate="txtAddNamaLengkap" Display="Dynamic" ErrorMessage="Isi Jenis Peraturan" ValidationGroup="addKonversi" CssClass="has-error help-block">
                                <prop:ClientSide.OnValidationError>
                                    $('divAddNamaLengkap').addClassName('has-error');
                                    jQuery('#<%=$this->FieldAddNamaValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAddNamaLengkap').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label text-right">Jenis Kelamin :</label>
                        <div class="col-lg-5">
                            <com:TRadioButton ID="rdAddPria" Checked="true" GroupName="rdaddJK"/>                        
                            <label>Pria</label>
                            <com:TRadioButton ID="rdAddWanita" GroupName="rdaddJK"/>                        
                            <label>Wanita</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label text-right">Status Perkawinan :</label>
                        <div class="col-lg-5">
                            <com:TRadioButton ID="rdAddSudahMenikah" Checked="true" GroupName="rdaddSdhNikah"/>                        
                            <label>Menikah</label>
                            <com:TRadioButton ID="rdAddBelumMenikah" GroupName="rdaddBlmNikah"/>                        
                            <label>Belum Menikah</label>
                        </div>
                    </div>  
                    <div class="form-group" id="divAddTempatLahir">
                        <label class="col-lg-2 control-label text-right">Tempat Lahir :</label>
                        <div class="col-lg-3">
                            <com:TTextBox ID="txtAddTempatLahir" CssClass="form-control" />
                            <com:TRequiredFieldValidator ID="FieldAddTempatLahirValidator" ControlToValidate="txtAddTempatLahir" Display="Dynamic" ErrorMessage="Isi Tempat Lahir" ValidationGroup="editformulir" CssClass="has-error help-block">
                                <prop:ClientSide.OnValidationError>
                                    $('divAddTempatLahir').addClassName('has-error');
                                    jQuery('#<%=$this->FieldAddTempatLahirValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAddTempatLahir').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label text-right">Tanggal Lahir :</label>
                        <div class="col-lg-4">
                            <com:TCustomDatePicker ID="txtAddTanggalLahir" DateFormat="dd-MMMM-yyyy" Culture="id"  FromYear="1940" UpToYear="<%= @date('Y') %>" InputMode="DropDownList" />
                        </div>
                    </div>
                    <div class="form-group" id="divAddAgama">
                        <label class="col-md-2 control-label text-right">Agama :</label>
                        <div class="col-md-2">
                            <com:TDropDownList ID="cmbAddAgama" CssClass="form-control" />                                                    
                            <com:TRequiredFieldValidator ID="FieldAddAgamaValidator" InitialValue="none" ControlToValidate="cmbAddAgama" Display="Dynamic" ErrorMessage="Pilih Agama yang dianut" ValidationGroup="" FocusOnError="true" CssClass="has-error help-block">			
                                <prop:ClientSide.OnValidationError>
                                    $('divAddAgama').addClassName('has-error');
                                    jQuery('#<%=$this->FieldAddAgamaValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAddAgama').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>                  
                        </div>
                    </div>
                    <div class="form-group" id="divAddKotaPendidikanTerakhir">
                        <label class="col-lg-2 control-label text-right">Pendidikan Terakhir :</label>
                        <div class="col-lg-3">
                            <com:TTextBox ID="txtAddKotaPendidikanTerakhir" CssClass="form-control" />                            
                            <com:TRequiredFieldValidator ID="FieldAddKotaPendidikanTerakhirValidator" ControlToValidate="txtAddKotaPendidikanTerakhir" Display="Dynamic" ErrorMessage="Isi Kota saat Pendidikan Terakhir" ValidationGroup="editformulir" CssClass="has-error help-block">
                                <prop:ClientSide.OnValidationError>
                                    $('divAddKotaPendidikanTerakhir').addClassName('has-error');
                                    jQuery('#<%=$this->FieldAddKotaPendidikanTerakhirValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAddKotaPendidikanTerakhir').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>
                    <div class="form-group" id="divAddPekerjaan">
                        <label class="col-lg-2 control-label text-right">Pekerjaan :</label>
                        <div class="col-lg-4">
                            <com:TDropDownList ID="cmbAddPekerjaan" CssClass="form-control"/>
                            <com:TRequiredFieldValidator ID="FieldAddPekerjaanValidator" InitialValue="none" ControlToValidate="cmbAddPekerjaan" Display="Dynamic" ErrorMessage="Pilih Pekerjaan Orangtua." ValidationGroup="editformulir" CssClass="has-error help-block">
                                <prop:ClientSide.OnValidationError>
                                    $('divAddPekerjaan').addClassName('has-error');
                                    jQuery('#<%=$this->FieldAddPekerjaanValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAddPekerjaan').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div> 
                    <div class="form-group">
                        <label class="col-lg-2 control-label text-right">Dapat Membaca Huruf :</label>
                        <div class="col-lg-4">
                            <com:TRadioButton ID="rdAddYa" Checked="true" GroupName="rdaddBaca"/>                        
                            <label>Ya</label>
                            <com:TRadioButton ID="rdAddTidak" GroupName="rdaddBaca"/>                        
                            <label>Tidak</label>
                        </div>
                    </div>
                    <div class="form-group" id="divAddKewarganegaraan">
                        <label class="col-md-2 control-label text-right">Kewarganegaraan :</label>
                        <div class="col-md-2">
                            <com:TDropDownList ID="cmbAddKewarganegaraan" CssClass="form-control" />                                                    
                            <com:TRequiredFieldValidator ID="FieldAddKewarganegaraanValidator" InitialValue="none" ControlToValidate="cmbAddKewarganegaraan" Display="Dynamic" ErrorMessage="Pilih Agama yang dianut" ValidationGroup="" FocusOnError="true" CssClass="has-error help-block">			
                                <prop:ClientSide.OnValidationError>
                                    $('divAddKewarganegaraan').addClassName('has-error');
                                    jQuery('#<%=$this->FieldAddKewarganegaraanValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAddKewarganegaraan').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>                  
                        </div>
                    </div>
                    <div class="form-group" id="divAddAlamatLengkap">
                        <label class="col-md-2 control-label text-right">Alamat Lengkap :</label>
                        <div class="col-md-4">
                            <com:TTextBox ID="txtAddAlamatLengkap" CssClass="form-control" />
                            <com:TRequiredFieldValidator ID="FieldAddAlamatLengkapValidator" ControlToValidate="txtAddAlamatLengkap" Display="Dynamic" ErrorMessage="Isi Alamat Tempat Tinggal" ValidationGroup="editformulir" CssClass="has-error help-block">
                                <prop:ClientSide.OnValidationError>
                                    $('divAddAlamatLengkap').addClassName('has-error');
                                    jQuery('#<%=$this->FieldAddAlamatLengkapValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAddAlamatLengkap').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>  
                    <div class="form-group" id="divAddKedudukan">
                        <label class="col-md-2 control-label text-right">Kedudukan Dalam Keluarga :</label>
                        <div class="col-md-2">
                            <com:TDropDownList ID="cmbAddKedudukan" CssClass="form-control" />                                                    
                            <com:TRequiredFieldValidator ID="FieldAddKedudukanValidator" InitialValue="none" ControlToValidate="cmbAddKedudukan" Display="Dynamic" ErrorMessage="Pilih Agama yang dianut" ValidationGroup="" FocusOnError="true" CssClass="has-error help-block">			
                                <prop:ClientSide.OnValidationError>
                                    $('divAddKedudukan').addClassName('has-error');
                                    jQuery('#<%=$this->FieldAddKedudukanValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAddKedudukan').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>                  
                        </div>
                    </div>
                    <div class="form-group" id="divAddNIK">
                        <label class="col-md-2 control-label text-right">NIK :</label>
                        <div class="col-md-4">
                            <com:TTextBox ID="txtAddNIK" CssClass="form-control" />
                            <com:TRequiredFieldValidator ID="FieldAddNIKValidator" ControlToValidate="txtAddNIK" Display="Dynamic" ErrorMessage="Isi Alamat Tempat Tinggal" ValidationGroup="editformulir" CssClass="has-error help-block">
                                <prop:ClientSide.OnValidationError>
                                    $('divAddNIK').addClassName('has-error');
                                    jQuery('#<%=$this->FieldAddNIKValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAddNIK').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>  
                    <div class="form-group" id="divAddNoKK">
                        <label class="col-md-2 control-label text-right">Nomor KK :</label>
                        <div class="col-md-4">
                            <com:TTextBox ID="txtAddNoKK" CssClass="form-control" />
                            <com:TRequiredFieldValidator ID="FieldAddNoKKValidator" ControlToValidate="txtAddNoKK" Display="Dynamic" ErrorMessage="Isi Alamat Tempat Tinggal" ValidationGroup="editformulir" CssClass="has-error help-block">
                                <prop:ClientSide.OnValidationError>
                                    $('divAddNoKK').addClassName('has-error');
                                    jQuery('#<%=$this->FieldAddNoKKValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAddNoKK').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>  
                    <div class="form-group" id="divAddKeterangan">
                        <label class="col-md-2 control-label text-right">Keterangan :</label>
                        <div class="col-md-5">
                            <com:TTextBox ID="txtAddKeterangan" CssClass="form-control" />
                            <com:TRequiredFieldValidator ID="FieldAddKeterangan" ControlToValidate="txtAddKeterangan" Display="Dynamic" ErrorMessage="Isi Nama PS Asal" ValidationGroup="addKonversi" CssClass="has-error help-block">
                                <prop:ClientSide.OnValidationError>
                                    $('divAddKeterangan').addClassName('has-error');
                                    jQuery('#<%=$this->FieldAddKeterangan->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAddKeterangan').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div> 
                </div>
            </div>
            <div class="panel-body">
                <div class="alert bg-warning alert-styled-right">
                    <button type="button" class="close"><span>&times;</span><span class="sr-only">Close</span></button>
                    <span class="text-semibold">Warning!</span> Kami blah.....
                </div>
            </div>
            <div class="table-responsive">                        
                <com:TRepeater ID="RepeaterAddS">	
                    <prop:HeaderTemplate>
                        <table class="table table-striped table-hover">
                            <thead>		
                                <tr class="bg-teal-700">
                                    <th width="20">NO</th>
                                    <th width="50">KODE</th>
                                    <th width="250">NAMA</th>
                                    <th width="30">SKS</th>
                                    <th width="30">SMT</th>
                                    <th width="80">KODE <br />MATKUL. Asal</th>
                                    <th width="250">MATAKULIAH ASAL</th>
                                    <th width="70">SKS</th>
                                    <th width="70">NILAI</th>                                                                        
                                </tr>
                            </thead>                                
                            <tbody>			
                                </prop:HeaderTemplate>
                            <prop:ItemTemplate>
                                <tr>
                                    <td><%# $this->DataItem['no']%></td>
                                    <td><%# $this->Page->Nilai->getKMatkul($this->DataItem['kmatkul'])%></td>
                                    <td><%# $this->DataItem['nmatkul']%></td>
                                    <td><%# $this->DataItem['sks']%></td>
                                    <td><%# $this->DataItem['semester']%></td>
                                    <td>
                                <com:TTextBox ID="txtKMatkulAsal" Text="<%# $this->DataItem['kmatkul_asal']%>" CssClass="form-control" />
                                </td>
                                <td>
                                <com:TTextBox ID="txtMatkulAsal" Text="<%# $this->DataItem['matkul_asal']%>" CssClass="form-control" />
                                </td>
                                <td>
                                <com:TTextBox ID="txtSksAsal" MaxLength="2" Text="<%# $this->DataItem['sks_asal']%>" CssClass="form-control" Attributes.OnKeyUp="formatangka(this)"/>
                                </td>
                                <td>
                                <com:THiddenField ID="hiddenidnilaikonversi" Value="<%# $this->DataItem['idnilai_konversi']%>"  />
                                <com:TDropDownList ID="cmbNilaiAsal" Text="<%# $this->DataItem['n_kual']%>" CssClass="form-control">
                                    <com:TListItem Value="none" Text=" " />
                                    <com:TListItem Value="A" Text="A" />
                                    <com:TListItem Value="B" Text="B" />
                                    <com:TListItem Value="C" Text="C" />
                                    <com:TListItem Value="D" Text="D" />											
                                </com:TDropDownList>
                                </td>                                                                       
                                </tr>
                            </prop:ItemTemplate>
                            <prop:FooterTemplate>
                                </tbody>
                                <tfoot>
                                    <tr>
                                        <td colspan="9">                                            	
                                <com:TActiveCustomButton ID="btnSaveRepeater" CssClass="btn btn-info btn-labeled btn-xs" OnClick="Page.saveData" ValidationGroup="addKonversi">
                                    <prop:Text>                                        
                                        <b><i class="icon-floppy-disk "></i></b> Simpan                                        
                                    </prop:Text>
                                    <prop:ClientSide.OnPreDispatch>                                                                   
                                        Pace.stop();
                                        Pace.start(); 
                                        $('<%=$this->btnSaveRepeater->ClientId%>').disabled='disabled';						
                                    </prop:ClientSide.OnPreDispatch>
                                    <prop:ClientSide.OnLoading>
                                        $('<%=$this->btnSaveRepeater->ClientId%>').disabled='disabled';						
                                    </prop:ClientSide.OnLoading>
                                    <prop:ClientSide.OnComplete>																	                                    						                                                                            
                                        $('<%=$this->btnSaveRepeater->ClientId%>').disabled='';
                                    </prop:ClientSide.OnComplete>
                                </com:TActiveCustomButton>                               
                                </td>
                                </tr>
                                </tfoot>
                        </table>
                        </prop:FooterTemplate>
                        <prop:EmptyTemplate>                                                       
                            <div class="panel-body">
                                <div class="alert alert-info alert-styled-left alert-bordered">
                                    <span class="text-semibold">Info!</span>
                                    Belum ada data yang bisa ditampilkan.
                                </div>                
                            </div>  
                        </prop:EmptyTemplate>
                </com:TRepeater>
            </div>
        </div>  
    </com:TPanel>
</com:TContent>